<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://jax.org/phenotype-data-sources"
            xmlns:tns="http://jax.org/phenotype-data-sources"
            elementFormDefault="qualified"
            xmlns:hap-anal="http://jax.org/haplotype-analysis-types">
    <xsd:import schemaLocation="../jax.org/haplotype-analysis-types.xsd"
                namespace="http://jax.org/haplotype-analysis-types"/>
    <xsd:element name="phenotypeDataSources">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="phenotypeDataSource"
                             type="hap-anal:phenotypeDataSourceType"
                             maxOccurs="unbounded">
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
